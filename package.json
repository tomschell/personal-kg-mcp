{
  "name": "personal-kg-mcp",
  "version": "2.0.0",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx src/server.ts",
    "dev:modular": "tsx src/server-modular.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:modular": "node dist/server-modular.js",
    "start:cjs": "node server.cjs",
    "test": "vitest run --config vitest.config.ts",
    "test:unit": "vitest run --config vitest.config.ts --reporter=verbose",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.16.0",
    "dotenv": "^16.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "tsx": "^4.20.3",
    "typescript": "^5.5.4",
    "vitest": "^2.1.8"
  }
  ,
  "publishConfig": { "access": "public" },
  "files": ["dist", "src", "server.cjs", "README.md", "LICENSE"],
  "keywords": ["mcp", "knowledge-graph", "personal-kg", "model-context-protocol"],
  "author": "Thomas Schell",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomschell/personal-kg-mcp-v2.git"
  },
  "bugs": { "url": "https://github.com/tomschell/personal-kg-mcp-v2/issues" },
  "homepage": "https://github.com/tomschell/personal-kg-mcp-v2#readme"
}


